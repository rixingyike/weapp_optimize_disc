{
  "pages": [
    "index/pages/index/index"
  ],
  "subpackages": [
    {
      "root": "examples",
      "pages": [
        "pages/index/index",
        "pages/performance/index"
      ]
    },
    {
      "root": "user",
      "pages": [
        "pages/my/index"
      ]
    },
    {
